    "@angular/compiler": "^17.2.0",
    "@angular/core": "^17.2.0",
    "@angular/forms": "^17.2.0",
    "@angular/material": "^17.2.1",
    "@angular/material-moment-adapter": "^17.2.1",
    "@angular/platform-browser": "^17.2.0",
    "@angular/platform-browser-dynamic": "^17.2.0",
    "@angular/router": "^17.2.0",
    "@azure/msal-angular": "^3.0.13",
    "@azure/msal-browser": "^3.10.0",
    "@fortawesome/fontawesome-svg-core": "^6.5.1",
    "@fortawesome/free-brands-svg-icons": "^6.5.1",
    "@fortawesome/free-regular-svg-icons": "^6.5.1",
    "@fortawesome/free-solid-svg-icons": "^6.5.1",
    "@microsoft/applicationinsights-web": "^3.1.0",
    "@ng-bootstrap/ng-bootstrap": "^16.0.0",
    "@ngrx/component": "^17.1.1",
    "@ngrx/component-store": "^17.1.1",
    "@ngrx/effects": "^17.1.1",
    "@ngrx/store": "^17.1.1",
    "@ngrx/store-devtools": "^17.1.1",
    "angular-calendar": "^0.31.0",
    "bootstrap": "^5.3.3",
    "jspdf": "^2.5.1",
    "moment": "^2.30.1",
    "ngx-doc-viewer": "^15.0.1",
    "ngx-material-timepicker": "^13.1.1",
    "ngx-multiple-dates": "^17.0.1",
    "ngx-toastr": "^18.0.0",
    "rxjs": "~7.8.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.14.3"
