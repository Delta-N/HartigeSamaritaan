{
  "name": "roosterplanner-ui",
  "version": "$(versionNumber)",
  "scripts": {
    "ng": "node --max-old-space-size=8192 node_modules/@angular/cli/bin/ng",
    "build": "pnpm run ng build -c production --base-href /",
    "start": "pnpm run ng serve",
    "test": "pnpm run ng test",
    "test:dev": "pnpm run ng test --configuration development",
    "lint": "pnpm run ng lint --fix",
    "i18n": "pnpm run ng extract-i18n --output-path src/locales",
    "e2e": "pnpm run ng e2e",
    "precommit": "pnpm run lint",
    "postinstall": "ngcc"
  },
  "private": true,
  "dependencies": {
    "@angular-eslint/schematics": "^17.2.1",
    "@angular/animations": "^17.2.2",
    "@angular/cdk": "^17.2.1",
